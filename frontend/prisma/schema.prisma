// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Announcement {
  id      String @id @default(cuid())
  date    String
  content String
}

model Substitutions {
  id                 String @id @default(cuid())
  date               String
  group              String
  hour               String
  teacher            String
  substitutedteacher String
  lessoninfo         String
  room               String
}
